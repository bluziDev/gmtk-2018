absVInc=abs(vInc)
absHInc=abs(hInc)
if absH&gt;absV{
    vInc=(absV/absH)*vInc
}
if absV&gt;absH{
    hInc=(absH/absV)*hInc
}
for(var i=0;i&lt;spMax;i+=accSp){
